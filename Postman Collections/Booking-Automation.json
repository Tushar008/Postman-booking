				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"// A common function to set checkin and checkout dates relative to the current date.\r",
					"// Accepts optional 'daysOffset' for checkin relative to today.\r",
					"// Accepts optional 'stayDuration' in days.\r",
					"pm.globals.set(\"setBookingDates\", function (checkinOffsetDays = 0, stayDurationDays = 1) {\r",
					"    function formatDate(date) {\r",
					"        const year = date.getFullYear();\r",
					"        const month = (date.getMonth() + 1).toString().padStart(2, '0');\r",
					"        const day = date.getDate().toString().padStart(2, '0');\r",
					"        return `${year}-${month}-${day}`;\r",
					"    }\r",
					"\r",
					"    const today = new Date();\r",
					"\r",
					"    // Calculate Check-in Date\r",
					"    const checkInDateObj = new Date(today);\r",
					"    checkInDateObj.setDate(today.getDate() + checkinOffsetDays);\r",
					"    const checkInDateFormatted = formatDate(checkInDateObj);\r",
					"\r",
					"    // Calculate Check-out Date\r",
					"    const checkOutDateObj = new Date(checkInDateObj);\r",
					"    checkOutDateObj.setDate(checkInDateObj.getDate() + stayDurationDays);\r",
					"    const checkOutDateFormatted = formatDate(checkOutDateObj);\r",
					"\r",
					"    // Set variables for use in the request body\r",
					"    // Using environment variables is usually best; adjust if you prefer global or collection variables.\r",
					"    pm.environment.set(\"checkin_date\", checkInDateFormatted);\r",
					"    pm.environment.set(\"checkout_date\", checkOutDateFormatted);\r",
					"\r",
					"    console.log(`Set dates: Check-in: ${checkInDateFormatted}, Check-out: ${checkOutDateFormatted}`);\r",
					"});\r",
					""
				]
			}
		},
